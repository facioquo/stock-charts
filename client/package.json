{
  "name": "@stock-charts/client",
  "description": "Angular frontend for stock charts application",
  "type": "module",
  "packageManager": "pnpm@10.27.0",
  "engines": {
    "node": ">=24.12.0",
    "pnpm": ">=10.27.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open --hmr",
    "prebuild": "node ../scripts/generate-backup-indicators.js",
    "build": "ng build --configuration=development",
    "prebuild.prod": "node ../scripts/generate-backup-indicators.js",
    "build.prod": "ng build --configuration=production",
    "build:prod": "pnpm run build.prod",
    "watch": "ng build --configuration=development --watch",
    "test": "ng test --watch=false",
    "test:watch": "ng test --watch",
    "test:coverage": "ng test --watch=false --coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write \"src/**/*.{ts,html,scss,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,css,json}\"",
    "clean": "rm -rf dist .angular node_modules/.cache"
  },
  "dependencies": {
    "@angular/animations": "21.0.3",
    "@angular/cdk": "21.0.1",
    "@angular/common": "21.0.3",
    "@angular/compiler": "21.0.3",
    "@angular/core": "21.0.3",
    "@angular/forms": "21.0.3",
    "@angular/material": "21.0.1",
    "@angular/platform-browser": "21.0.3",
    "@angular/platform-browser-dynamic": "21.0.3",
    "@angular/router": "21.0.3",
    "@angular/service-worker": "21.0.3",
    "@ctrl/tinycolor": "4.2.0",
    "@ng-matero/extensions": "20.3.1",
    "chart.js": "4.5.1",
    "chartjs-adapter-date-fns": "3.0.0",
    "chartjs-plugin-annotation": "3.1.0",
    "date-fns": "4.1.0",
    "ngx-color": "10.1.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "uuid": "11.1.0",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "21.0.2",
    "@angular/cli": "21.0.2",
    "@angular/compiler-cli": "21.0.3",
    "@eslint/js": "9.39.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/eslint-plugin": "^1.5.1",
    "angular-eslint": "21.0.1",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.4",
    "jsdom": "26.1.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "vitest": "4.0.15"
  }
}
