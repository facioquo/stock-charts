{
  "servers": {
    "angular-cli": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y","@angular/cli@latest","mcp"]
    },
    "cloudflare": {
      "type": "stdio",
      "command": "npx",
      "args": ["mcp-remote", "https://docs.mcp.cloudflare.com/mcp"]
    },
    "context7": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y","@upstash/context7-mcp@latest"]
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "X-MCP-Toolsets": "default,actions,projects,copilot,github_support_docs_search,web_search",
        "X-MCP-Readonly": "true"
      }
    },
    "mslearn": {
      "type": "http",
      "url": "https://learn.microsoft.com/api/mcp"
    },
    "next-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y","playwright","run-test-mcp-server"]
    },
    "primer": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y","@primer/mcp@latest"]
    }
  }
}
